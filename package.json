{
  "name": "onecx-tenant-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "apigen": "openapi-generator-cli generate -i src/assets/swagger/onecx-tenant-bff.yaml -c apigen.yaml -o src/app/shared/generated -g typescript-angular --type-mappings AnyType=object",
    "start": "nx serve",
    "prepare": "husky install",
    "build": "nx build --configuration=production",
    "format": "nx format:write --uncommitted",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "sonar": "npx sonarqube-scanner -Dproject.settings=sonar-local-project.properties",
    "test": "nx test",
    "test:ci": "nx test --watch=false --browsers=ChromeHeadless --code-coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^15.0.0",
