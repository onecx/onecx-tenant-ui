    "@angular/router": "^15.2.7",
    "@ngrx/component": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@onecx/accelerator": "^4.10.2",
    "@onecx/integration-interface": "^4.10.2",
    "@onecx/keycloak-auth": "^4.10.2",
    "@onecx/nx-plugin": "1.2.0",
    "@onecx/portal-integration-angular": "^4.23.0",
    "@onecx/portal-layout-styles": "^4.10.2",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "15.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.22.1",
    "zone.js": "~0.12.0"
